# 세션 설정 (둘 중 하나만 사용; COOKIE_TANK 우선 권장)
# CHROME_PROFILE=C:\Users\lee\AppData\Local\Google\Chrome\User Data\Profile 1
# COOKIE_TANK=cookies.json

# 출력 설정
OUT_DIR=out

# 동시성 및 속도 제한
CONCURRENCY=2
QPS=2
TIMEOUT_MS=120000

# 수집 설정
MAX_ITEMS=16
CODES=AP,REG,BLD,ZON,RS,TEN,RTR,NT,NOI,IMG

# 실행 설정
HEADLESS=false
LOG_LEVEL=INFO

# Chrome profile path (optional, uses cookies.json if not set)
CHROME_PROFILE=

# Storage state file for session management
COOKIE_TANK=cookies.json

# OS downloads folder fallback (optional)
OS_DOWNLOADS_FALLBACK=false
OS_DOWNLOADS_DIR=C:\Users\lee\Downloads

# 고급 설정 (일반적으로 변경 불필요)
# HOOK_MS=600
# MAX_RETRIES=3